        "archiver": "^3.0.0",
        "babel-plugin-module-resolver": "^3.2.0",
        "cross-env": "^5.2.0",
        "crypto": "^1.0.1",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "gulp": "^4.0.2",
