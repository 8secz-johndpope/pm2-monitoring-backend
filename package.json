{
    "name": "pm2-monitoring-backend",
    "author": "liangxiaojun",
    "version": "1.0.0",
    "description": "PM2 monitoring Back-end Project",
    "main": "./src/app.js",
    "scripts": {
        "dev": "nodemon $NODE_DEBUG_OPTION src/app --exec babel-node --ignore 'src/pm2-apps/*'",
        "clean": "rimraf dist",
        "package": "cross-env NODE_ENV=production gulp package",
        "build": "npm run clean && npm run package",
        "run": "cross-env NODE_ENV=production node $NODE_DEBUG_OPTION dist/app",
        "start": "npm run build && npm run run",
        "zip": "npm run build && node build/zip"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/fatalxiao/pm2-monitoring-backend"
    },
    "keywords": [
        "pm2",
        "monitoring",
        "koa2"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/fatalxiao/pm2-monitoring-backend/issues"
    },
    "dependencies": {
        "@koa/cors": "^3.0.0",
        "koa": "^2.7.0",
        "koa-body": "^4.1.0",
        "koa-bodyparser": "^4.2.1",
        "koa-router": "^7.4.0",
        "koa-static": "^5.0.0",
        "koa-websocket": "^5.0.1",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "mysql2": "^1.6.5",
        "pm2": "^3.5.1",
        "request": "^2.88.0",
        "request-promise": "^4.2.4",
        "sequelize": "^5.8.7",
